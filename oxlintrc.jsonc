{
  "$schema": "./node_modules/oxlint/configuration_schema.json",
  "plugins": [
    "typescript",
    "unicorn",
    "react",
    "react-perf",
    "import",
    "jsx-a11y",
    "node",
    "promise",
    "vitest",
    "vue",
  ],
  "ignorePatterns": [
    "*.min.*",
    "*.map",
    "**/public",
    "**/build",
    "**/dist",
    "**/out",
    "**/.github",
    "**/.next",
    "**/.astro",
    "**/.netlify",
  ],
}
